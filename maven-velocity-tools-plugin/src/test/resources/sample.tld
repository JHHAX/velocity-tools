<?xml version="1.0" encoding="UTF-8"?>
<taglib
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd"
  xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  version="2.1">
   <description>This is a sample.</description>
   <tlib-version>1.2</tlib-version>
   <short-name>sample1</short-name>
   <uri>http://velocity.apache.org/jsp/sample1</uri>
   <tag>
      <description>My classic tag</description>
      <name>myClassicTag</name>
      <tag-class>org.apache.velocity.tools.plugin.taglib.example.MyClassicTag</tag-class>
      <body-content>scriptless</body-content>
      <attribute>
         <description>String property</description>
         <name>stringProperty</name>
         <required>false</required>
         <rtexprvalue>true</rtexprvalue>
         <type>java.lang.String</type>
      </attribute>
      <attribute>
         <description>int property</description>
         <name>intProperty</name>
         <required>false</required>
         <rtexprvalue>true</rtexprvalue>
         <type>int</type>
      </attribute>
   </tag>
   <tag>
      <description>My bodyless classic tag</description>
      <name>myBodylessClassicTag</name>
      <tag-class>org.apache.velocity.tools.plugin.taglib.example.MyBodylessClassicTag</tag-class>
      <body-content>empty</body-content>
      <attribute>
         <description>String property</description>
         <name>stringProperty</name>
         <required>false</required>
         <rtexprvalue>true</rtexprvalue>
         <type>java.lang.String</type>
      </attribute>
      <attribute>
         <description>int property</description>
         <name>intProperty</name>
         <required>false</required>
         <rtexprvalue>true</rtexprvalue>
         <type>int</type>
      </attribute>
   </tag>
   <tag>
      <description>My simple tag</description>
      <name>mySimpleTag</name>
      <tag-class>org.apache.velocity.tools.plugin.taglib.example.MySimpleTag</tag-class>
      <body-content>scriptless</body-content>
      <attribute>
         <description>String property</description>
         <name>stringProperty</name>
         <required>false</required>
         <rtexprvalue>true</rtexprvalue>
         <type>java.lang.String</type>
      </attribute>
      <attribute>
         <description>int property</description>
         <name>intProperty</name>
         <required>false</required>
         <rtexprvalue>true</rtexprvalue>
         <type>int</type>
      </attribute>
   </tag>
</taglib>
